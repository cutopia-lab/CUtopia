{
  "name": "graphql-endpoint",
  "version": "1.0.0",
  "description": "",
  "main": "graphql.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "yarn run build && nodemon build/graphql.js",
    "local": "yarn run build-db && yarn run build && node build/local.js",
    "build-db": "yarn --cwd ../../mongodb run build && yarn run postdeploy",
    "predeploy": "yarn run build-db && yarn run build && rm -rf node_modules && yarn install --production && cp -RL node_modules/ build/",
    "postdeploy": "rm -rf node_modules && yarn install",
    "copyfile": "sh copyfile.sh",
    "build": "rm -rf build && tsc && yarn run copyfile"
  },
