    "fuse.js": "^6.4.6",
    "graphql": "^15.4.0",
    "graphql-scalars": "^1.10.0",
    "graphql-tools": "^7.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
