{
  "name": "graphql-endpoint",
  "version": "1.0.0",
  "description": "",
  "main": "graphql.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-schema": "ts-node src/schemas/build.ts",
    "build-gql-types": "graphql-codegen --config codegen.yml",
    "build": "rm -rf build && yarn run build-schema && tsc && sh copyfile.sh",
    "predeploy": "yarn run build && rm -rf node_modules && rm -rf build/node_modules && yarn install --prod && cp -RL node_modules/ build/",
    "postdeploy": "rm -rf node_modules && yarn install --prod=false",
    "local": "nodemon build/local.js",
    "local-gql": "yarn run build-schema && yarn run build-gql-types && sh copyfile.sh",
    "upgrade-db": "yarn upgrade mongodb && cp -RL node_modules/mongodb/ build/node_modules/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^6.5.3",
    "apollo-server-lambda": "^3.8.1",
    "aws-sdk": "^2.1144.0",
    "cutopia-types": "^1.0.23",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "graphql-constraint-directive": "^3.1.1",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.1.28",
    "graphql-rate-limit": "^3.3.0",
    "graphql-scalars": "^1.17.0",
    "graphql-tools": "^8.2.11",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "file:../../mongodb/lib",
    "node-cache": "^5.1.2"
  },
  "engines": {
    "node": ">=v14.0.0",
    "npm": ">=6.14.8",
    "yarn": ">=1.22.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-document-nodes": "2.2.11",
    "@graphql-codegen/typescript-resolvers": "^2.6.4",
    "@types/jsonwebtoken": "^8.5.8",
    "apollo-server-express": "^3.8.1",
    "express": "^4.18.1",
    "ts-node": "^10.8.0",
    "webpack": "^5.72.1"
  }
}
