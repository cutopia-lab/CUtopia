  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "@mui/styles": "^5.10.2",
    "@sentry/nextjs": "^7.8.0",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "cutopia-types": "^1.0.29",
    "graphql": "^15.5.2",
    "html2canvas": "^1.4.1",
    "lodash": "^4.17.21",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "next": "12.1.6",
    "next-seo": "^5.5.0",
    "nprogress": "^0.2.0",
    "pluralize": "^8.0.0",
    "react": "^17.0.2",
