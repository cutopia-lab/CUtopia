{
  "name": "cutopia",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=v12.21.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@sentry/nextjs": "^7.8.0",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "cutopia-types": "^1.0.28",
    "graphql": "^15.5.2",
    "lodash": "^4.17.21",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "next": "^12.1.0",
    "nprogress": "^0.2.0",
    "pluralize": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-icons": "^4.2.0",
    "react-use": "^17.3.1"
  },
  "scripts": {
    "start": "next start",
    "restart": "rm -f node_modules/.cache/.eslintcache && rm -f ./eslintcache && yarn start --reset-cache",
    "build": "next build && next export -o build",
    "postbuild": "next-sitemap",
    "test": "jest",
    "test-init": "ts-jest config:init",
    "export": "next export -o build",
    "serve": "serve build",
    "dev": "next dev",
    "eject": "next eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "prepare": "husky install && yarn run update-data",
    "cleanup": "rm -r .next build node_modules out && yarn install && yarn run dev",
    "update-data": "git submodule update --init && cp -R data/resources public/",
    "clear-console": "gulp strip-debug"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/loader-utils": "^2.0.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.7.10",
    "@types/nprogress": "^0.2.0",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^17.0.19",
    "@types/react-beforeunload": "^2.1.1",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "husky": "^7.0.2",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": ">=11.1.2",
    "loader-utils": "^3.2.0",
    "next-sitemap": "^3.1.15",
    "prettier": "^2.3.2",
    "sass": "^1.54.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.2",
    "webpack": "^5.74.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
